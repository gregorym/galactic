{
  "name": "Galactic",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "permissions": [
    "https://github.com/stars"
  ],
"content_scripts": [
    {
      "matches": ["https://github.com/stars"],
      "js": [
        "lib/jquery.js",
        "lib/underscore.js",
        "lib/backbone.js",
        "lib/handlebar.js",
        "lib/github.js",
        
        "galactic.js",

        "models/user.js",
        "models/repo.js",

        "collections/repos.js",

        "views/stars/index.js",

        "page.js"
      ]
    }
  ]
}